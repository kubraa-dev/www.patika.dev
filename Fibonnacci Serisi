public class deneme {
    public static void main(String[] args) {
    
        int n = 9, n1 = 0, n2 = 1, n3, total = 0;

        System.out.println(n1 + " \n " + n2);//0 ve 1 yazdÄ±r

        for (int i = 2; i <= n; i++) {
            n3 = n1 + n2;
            System.out.println(" " + n3);
            n1 = n2;
            n2 = n3;
        }
        System.out.println();
    }
}

