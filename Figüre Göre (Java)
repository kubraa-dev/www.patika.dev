import java.util.Scanner;

public class calisma {

    static void figur(){

        Scanner inp = new Scanner(System.in);

        System.out.println("Bir sayı giriniz: ");
        int n = inp.nextInt();

        int ilksayi = n;

        for(int i = 0; i < n; i -= 5){
            ilksayi -= 5;
        }
        System.out.println("Çıktı:"+n+" ");

    }
}
